@using Booking_Backend.Repository.Hotels.Request;
@using Booking_Frontend.WebApp.Models.Owner;
@model InfoOwnerRegisterViewModel;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Xác nhận đăng kí bán  phòng</title>
    <link rel="stylesheet"
          href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
          integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z"
          crossorigin="anonymous" />
</head>
<body>
    <div class="container mt-5">
        <h1 class="text-center mb-5">Xin chào @(Model.OwnerName), Gonow.net xin gửi lời cảm ơn đến quý khách!</h1>
        <p class="lead text-center">
            Yêu cầu bán phòng của bạn đã được gửi đi. Chúng tôi sẽ phản hồi với bạn qua email trễ nhất là 24h. Hãy luôn cập nhật thông tin nhé!
        </p>
        <div class="d-flex justify-content-center mt-5">
            <a href="/" class="btn btn-primary">Trang chủ</a>
        </div>
    </div>
</body>
</html>